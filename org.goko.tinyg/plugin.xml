<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="always"
            uri="fragments/TinyGFragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferencePage">
      <page
            class="org.goko.tinyg.configuration.TinyGPreferencesPage"
            id="org.goko.tinyg.page2"
            name="TinyG">
      </page>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferenceStoreProvider">
      <provider
            class="org.goko.tinyg.controller.prefs.TinyGPreferenceStoreProvider"
            id="org.goko.tinyg.provider">
      </provider>
   </extension>

</plugin>
