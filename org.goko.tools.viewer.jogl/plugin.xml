<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="initial"
            uri="fragments/gcodeViewer.e4xmi">
      </fragment>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferencePage">
      <page
            class="org.goko.tools.viewer.jogl.preferences.performances.JoglViewerPreferencePage"
            id="org.goko.jogl.viewer.preferences.viewer"
            name="Viewer">
      </page>
      <page
            category="org.goko.jogl.viewer.preferences.viewer"
            class="org.goko.tools.viewer.jogl.preferences.rotaryaxis.JoglRotaryAxisPreferencePage"
            id="org.goko.jogl.viewer.preferences.4thaxis"
            name="4th axis">
      </page>
      <page
            category="org.goko.jogl.viewer.preferences.viewer"
            class="org.goko.tools.viewer.jogl.preferences.GridPreferencesPage"
            id="org.goko.jogl.viewer.preferences.grid"
            name="Grid">
      </page>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferenceStoreProvider">
      <provider
            class="org.goko.tools.viewer.jogl.preferences.JoglViewerPreferenceStoreProvider"
            id="org.goko.tools.viewer.provider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.goko.tools.viewer.jogl.preferences.JoglViewerPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
